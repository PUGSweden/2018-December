  "InventoryService": [
    {
      "Id": 3,
      "Name": "Check my group membership",
      "Description": "Gets all groups that a user is a member of.",
      "Command": "function Get-InventoryServiceInstances \n{\n    param (\n    [string]$parentId,\n    [string[]]$inventoryKeys\n    )\n    \n    $Domain, $Object = $parentId -split '\\\\'\n    \n    $CurrentObject = Get-ADObject -Filter \"SamAccountName -eq \"\"$Object\"\"\" -Properties MemberOf | Select-Object -ExpandProperty MemberOf | sort-object\n    \n    foreach ($group in $CurrentObject) {\n        $GroupName = ($group -split ',')[0].TrimStart('CN=')\n        $inventoriedServiceInstance = New-InventoryServiceInstance -InventoryKey 'ADGroup' -Identifier $GroupName -ServiceName 'AD Group:'\n        write-host \"group: $group\"\n        write-host \"groupname: $GroupName\"\n        write-host \"parentID: $parentId\"\n\n        $inventoriedServiceInstance | Add-InstanceAttribute -Name 'GroupName' -Value $GroupName -Primary $true \n        $inventoriedServiceInstance | Add-InstanceAttribute -Name 'ObjectName' -Value $parentId\n        $inventoriedServiceInstance | Add-InstanceAttribute -Name 'group' -Value $group -hidden $true\n    \n        $inventoriedServiceInstance\n    }\n}\n",
      "Active": true,
      "MissitoId": "51b8f78a-c3d9-4865-b51c-d9ca578c958d"
    }
  ],
